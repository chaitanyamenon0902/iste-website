generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  id        String   @id @default(cuid())
  name          String
  description   String?
  image_url     String?
  sig           String
  year          Int
  keywords      String[]
  github_link   String?
  simulation_link String?
  tackled       String?
  approach      String?
  mentors       String[]
  mentees       String[]
}

